<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1>Ho trovato <%= numOfDresses %> vestiti</h1>
        </div>
    </div>
</div>


<div class="container">
    <div class="row">
        <div class="col-md-12">
            <% listOfDresses.forEach(function(dress) { %>
            <div>
                <h3><%= dress.id %> - <%= dress.name %></h3>
                <p><%= dress.season %></p>
                <img src="/images/<%= dress.picture %>" width=200>
            </div>
            <% }) %>
        </div>
    </div>
</div>

